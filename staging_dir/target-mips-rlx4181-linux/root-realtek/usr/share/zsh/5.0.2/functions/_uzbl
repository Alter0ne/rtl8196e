  '(-u --uri)'{-u,--uri=}':uri to load:_urls' \
  '(-v --verbose)'{-v,--verbose}'[verbose]' \
  '(-n --name)'{-n,--name=}':instance name' \
  '(-c --config)'{-c,--config=}':config file:_files' \
  '--display=:X display:_x_display' \
  '--help[help]'

