		yes-no)
		    echo "$as_me:$LINENO: result: yes on SMP build, but not on non-SMP build" >&5
echo "${ECHO_T}yes on SMP build, but not on non-SMP build" >&6;;
		*-yes)

cat >>confdefs.h <<\_ACEOF
