    $libdir/UNIVERSAL.3 \
    $libdir/Unicode::Collate.3 \
    $libdir/Unicode::Collate::CJK::Big5.3 \
    $libdir/Unicode::Collate::CJK::GB2312.3 \
    $libdir/Unicode::Collate::CJK::JISX0208.3 \
    $libdir/Unicode::Collate::CJK::Korean.3 \
    $libdir/Unicode::Collate::CJK::Pinyin.3 \
