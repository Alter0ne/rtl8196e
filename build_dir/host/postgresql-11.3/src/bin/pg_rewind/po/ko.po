#: pg_rewind.c:66
#, c-format
msgid ""
"%s resynchronizes a PostgreSQL cluster with another copy of the cluster.\n"
"\n"
msgstr ""
"%s 프로그램은 PostgreSQL 한 클러스터에서 다른 클러스터로 재동기화 하는 도구입니다.\n"
"\n"

#: pg_rewind.c:67
#, c-format
msgid ""
"Usage:\n"
"  %s [OPTION]...\n"
"\n"
msgstr ""
"사용법:\n"
"  %s [옵션]...\n"
"\n"

#: pg_rewind.c:68
#, c-format
msgid "Options:\n"
msgstr "옵션들:\n"

#: pg_rewind.c:69
#, c-format
msgid "  -D, --target-pgdata=DIRECTORY  existing data directory to modify\n"
msgstr "  -D, --target-pgdata=디렉터리   변경하려는 데이터 디렉터리\n"

#: pg_rewind.c:70
#, c-format
msgid "      --source-pgdata=DIRECTORY  source data directory to synchronize with\n"
msgstr "      --source-pgdata=디렉터리   동기화 원본이 되는 데이터 디렉터리\n"

#: pg_rewind.c:71
#, c-format
msgid "      --source-server=CONNSTR    source server to synchronize with\n"
msgstr "      --source-server=연결문자열  원본 서버 접속 정보\n"

#: pg_rewind.c:72
#, c-format
msgid "  -n, --dry-run                  stop before modifying anything\n"
msgstr "  -n, --dry-run                  변경 작업 전에 멈춤(검사, 확인용)\n"

#: pg_rewind.c:73
#, c-format
msgid "  -P, --progress                 write progress messages\n"
msgstr "  -P, --progress                 진행 과정 메시지를 보여줌\n"

#: pg_rewind.c:74
#, c-format
msgid "      --debug                    write a lot of debug messages\n"
msgstr "      --debug                    디버그 메시지를 보여줌\n"

#: pg_rewind.c:75
#, c-format
msgid "  -V, --version                  output version information, then exit\n"
msgstr "  -V, --version                  버전 정보를 보여주고 마침\n"

#: pg_rewind.c:76
#, c-format
msgid "  -?, --help                     show this help, then exit\n"
msgstr "  -?, --help                     이 도움말을 보여주고 마침\n"

#: pg_rewind.c:77
#, c-format
msgid ""
"\n"
"Report bugs to <pgsql-bugs@postgresql.org>.\n"
msgstr ""
"\n"
"오류보고: <pgsql-bugs@postgresql.org>.\n"

#: pg_rewind.c:132 pg_rewind.c:163 pg_rewind.c:170 pg_rewind.c:177
#: pg_rewind.c:185
