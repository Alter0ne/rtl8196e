#: pg_basebackup.c:715
#, c-format
msgid "%s: directory \"%s\" exists but is not empty\n"
msgstr "%s: \"%s\" 디렉터리가 있지만 비어 있지 않음\n"

#: pg_basebackup.c:723
#, c-format
msgid "%s: could not access directory \"%s\": %s\n"
msgstr "%s: \"%s\" 디렉터리에 액세스할 수 없음: %s\n"

