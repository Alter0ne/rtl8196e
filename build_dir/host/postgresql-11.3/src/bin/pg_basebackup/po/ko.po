#: pg_basebackup.c:813
#, c-format
msgid "%*s/%s kB (%d%%), %d/%d tablespace"
msgid_plural "%*s/%s kB (%d%%), %d/%d tablespaces"
msgstr[0] "%*s/%s kB (%d%%), %d/%d 테이블스페이스"

#: pg_basebackup.c:838
#, c-format
