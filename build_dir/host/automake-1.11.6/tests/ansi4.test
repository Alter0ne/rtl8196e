# Quoting regression.  Test from Thomas Gagne.

. ./defs || Exit 1

set -e

cat >> configure.in << 'END'
AC_PROG_CC
AM_C_PROTOTYPES
END

cat > Makefile.am << 'END'
AUTOMAKE_OPTIONS = ansi2knr
bin_PROGRAMS = hello
hello_SOURCES = hello.c
END

: > ansi2knr.1
: > ansi2knr.c
: > hello.c

$ACLOCAL
$AUTOMAKE -Wno-obsolete

$FGREP 'ansi2knr.$(' Makefile.in
