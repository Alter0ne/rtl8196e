#, c-format
msgid "Error creating directory '%s': %s"
msgstr "Erro ao criar o diretório \"%s\": %s"

#: ../gio/gdbusauthmechanismsha1.c:377
#, c-format
msgid "Error opening keyring '%s' for reading: "
msgstr "Erro ao abrir o chaveiro \"%s\" para leitura: "

