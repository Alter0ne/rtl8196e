   BSD/OS 4.0.1 has a bug: <stddef.h>, <stdio.h> and <time.h> must be included
   before <wchar.h>.  */
#include <stddef.h>
#include <stdio.h>
#include <time.h>
#include <wchar.h>
#if !HAVE_DECL_WCWIDTH
extern
# ifdef __cplusplus
"C"
# endif
int wcwidth (int);
#endif
int main ()
{
  if (setlocale (LC_ALL, "fr_FR.UTF-8") != NULL)
    if (wcwidth (0x0301) > 0 || wcwidth (0x200B) > 0)
      return 1;
  return 0;
}
