( $at_check_trace; $LIBTOOL --mode=link --tag=CC $CC $CFLAGS $LDFLAGS -o bar/libbar.la bar/bar.lo 	 -L$libdir -lfoo -no-undefined -version-info 1:0:0 -rpath $libdir
