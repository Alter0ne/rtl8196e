prog_SOURCES = prog.c
prog_LDADD = $(sysroot)$(libdir)/lib2.la
_ATEOF


cat >prog/prog.c <<'_ATEOF'
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
extern char *g(const char *s);
int main()
{
  char *q = g("hello world");
  free (q);
  return 0;
}
_ATEOF


