libdir=$inst/lib
bindir=$inst/bin
mkdir $inst $libdir $bindir

{ set +x
$as_echo "$at_srcdir/help.at:61: \$LIBTOOL compile \$CC \$CPPFLAGS \$CFLAGS -c a.c"
