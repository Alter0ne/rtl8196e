$as_echo "$at_srcdir/sysroot.at:205: mkdir lib1 lib2 prog"
at_fn_check_prepare_trace "sysroot.at:205"
( $at_check_trace; mkdir lib1 lib2 prog
) >>"$at_stdout" 2>>"$at_stderr"
