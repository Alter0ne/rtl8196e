  $as_echo "120. $at_setup_line: testing $at_desc ..."
  $at_traceon



gcc_sysroot=`$CC --print-sysroot 2> /dev/null`
if test $? != 0; then
  gcc_sysroot=
fi
{ set +x
$as_echo "$at_srcdir/sysroot.at:205: test -n \"\$gcc_sysroot\" || exit 77"
