$at_failed && at_fn_log_failure
$at_traceon; }


conv=
for i in 1 2 3; do
  conv=$conv$i
  { set +x
$as_echo "$at_srcdir/convenience.at:275: \$LIBTOOL --tag=GCJ --mode=link \$GCJ \$GCJFLAGS \$LDFLAGS -static --main=foo\$i -o main_static\$EXEEXT foo\$i.lo liba\$conv.la"
